<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="tech.mathai.app.Mapper.ParentMapper">



    <select id="getStudents" resultType="tech.mathai.app.Entity.UserAttr">
      SELECT b.* FROM studentparent a,useraccount b WHERE a.parent_id=#{id} AND a.student_id=b.id
    </select>






</mapper>